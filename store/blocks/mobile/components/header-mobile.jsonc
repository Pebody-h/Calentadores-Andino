        "title": "Barra navegaci√≥n moviles",
        "children": [
            "logo#header__logo",
            "header-spacer",
            "drawer#header__menu--mobile"
        ],
        "props": {
            "sticky": true,
            "blockClass": "header__phone"
        }
    },
    "drawer#header__menu--mobile": {
        "title": "Menu Mobile",
        "children": [
            "menu#header__menu--mobile"
        ]
